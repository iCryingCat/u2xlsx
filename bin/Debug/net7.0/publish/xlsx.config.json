{
	"SourcePath": "./xlsx",
	"ExportPath": "./data",
	"ExportFlags": "lua",
	"LuaConfig": {
		"DataTableFormat": "XLSX_{0}_{1}",
		"DataTableObjectFormat": "I{0}",
		"LuaDefaultNameSpace": "global",
		"NameSpaceRegex": "@\\s*([a-zA-Z_][a-zA-Z0-9_]*)?\\s*",
		"IgnoreXlsxRegex": "#.*",
		"IgnoreSheetRegex": "#.*",
		"SheetRegex": "(Obj|Enum|Tbl)\\s*-\\s*([a-zA-Z_][a-zA-Z0-9_]*)",
		"LuaTypes": {
			"Number": "num",
			"String": "str",
			"ListNumber": "list:num",
			"ListString": "list:str",
			"InlineTable": "^([a-zA-Z_][a-zA-Z0-9_]*)\\s*::\\s*([a-zA-Z_][a-zA-Z0-9_]*)"
		}
	}
}
